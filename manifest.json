{
  "manifest_version": 2,
  "name": "Autoconsent Test",
  "version": "0.1",
  "background": {
    "scripts": [
      "test/background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["test/content.bundle.js"],
      "css": ["./dist/autoconsent.css"],
      "all_frames": true
    }
  ],
  "permissions": [
    "<all_urls>",
    "cookies",
    "tabs",
    "storage",
    "unlimitedStorage"
  ]
}
